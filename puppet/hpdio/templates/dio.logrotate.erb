<%= @install_dir %>/var/log/dionaea.log {
    weekly
    missingok
    rotate 52
    compress
    postrotate
        /etc/init.d/dio restart > /dev/null
    endscript
}

<%= @install_dir %>/var/dionaea/logsql.sqlite {
    yearly
    missingok
    rotate 10
    compress
    postrotate
        /etc/init.d/dio restart > /dev/null
    endscript
}
